{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "tsc && vite build",
    "lint": "bunx --bun eslint ./src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "bunx prettier --write 'src/**/*.{js,jsx,ts,tsx}' --log-level warn",
    "format:check": "bun x prettier 'src/**/*.{js,jsx,ts,tsx}' --check --log-level warn",
    "test": "bun test",
    "upgrade:all": "bunx npm-check-updates -ui"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/poppins": "^5.0.14",
    "@hookform/resolvers": "^3.9.0",
    "@mui/icons-material": "^5.16.5",
    "@mui/material": "^5.16.5",
    "@tanstack/react-query": "^5.51.15",
    "@tanstack/react-query-devtools": "5",
    "axios": "^1.7.2",
    "install": "^0.13.0",
    "js-cookie": "^3.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-router-dom": "^6.25.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^14.12.3",
    "@testing-library/react": "^16.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bun": "^1.1.6",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "bun-types": "^1.1.21",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.9",
    "jsdom": "^24.1.1",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vite": "^5.3.5"
  }
}
