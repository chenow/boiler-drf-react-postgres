{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "tsc && vite build",
    "lint": "bunx --bun eslint ./src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "bunx prettier --write 'src/**/*.{js,jsx,ts,tsx}' --log-level warn",
    "format:check": "bunx prettier --check 'src/**/*.{js,jsx,ts,tsx}' --log-level warn",
    "test": "bun test",
    "upgrade:all": "bunx npm-check-updates -ui"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@fontsource/poppins": "^5.0.12",
    "@hookform/resolvers": "^3.3.4",
    "@mui/icons-material": "^5.15.14",
    "@mui/material": "^5.15.14",
    "@tanstack/react-query": "^5.28.9",
    "@tanstack/react-query-devtools": "5",
    "axios": "^1.6.8",
    "install": "^0.13.0",
    "js-cookie": "^3.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.2",
    "react-router-dom": "^6.22.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^14.3.9",
    "@testing-library/react": "^14.2.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bun": "^1.0.11",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "bun-types": "^1.0.36",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3",
    "vite": "^5.2.7"
  }
}
